        "gulp-rename": "*",
        "gulp-uglify": "*",
        "gulp-tap": "*",
        "writefile": "*",
        "gulp-ui5-preload": "*",
        "gulp-pretty-data": "*",
        "gulp-minify": "*",
        "gulp-if": "*",
