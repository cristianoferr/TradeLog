{
    "_version": "1.3.0",
    "sap.app": {
        "_version": "1.3.0",
        "id": "tradelog",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "mainService": {
                "uri": "http://localhost:58760/odata/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "init_tiles": {
                "uri": "model/tiles.json",
                "type": "JSON"
            },
            "init_dominio": {
                "uri": "model/dominio.json",
                "type": "JSON"
            }
        }
    },
    "sap.ui": {
        "_version": "1.3.0",
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://detail-view",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "_version": "1.2.0",
        "rootView": {
            "viewName": "tradelog.view.App",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.36.0",
            "libs": {
                "sap.ui.core": {
                    "minVersion": "1.36.0"
                },
                "sap.m": {
                    "minVersion": "1.36.0"
                },
                "sap.ui.layout": {
                    "minVersion": "1.36.0"
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "tradelog.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "type": "sap.ui.model.odata.v4.ODataModel",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None"
                }
            },
            "dominio": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "init_dominio"
            },
            "tiles": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "init_tiles"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "content/css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "tradelog.domain",
                "controlId": "App",
                "controlAggregation": "pages",
                "transition": "slide",
                "bypassed": {
                    "target": [
                        "home",
                        "notFound"
                    ]
                }
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "home",
                    "target": [
                        "home"
                    ]
                },
                {
                    "pattern": "carteiras",
                    "name": "carteiras",
                    "target": "listaCarteiras"
                },
                {
                    "parent": "carteiras",
                    "pattern": "carteira/{carteira}",
                    "name": "carteira",
                    "target": "carteira"
                }
            ],
            "targets": {
                "splitscreenTarget": {
                    "viewName": "SplitScreen"
                },
                "masterDetailSimplesSplitApp": {
                    "viewPath": "tradelog.layout",
                    "viewName": "masterDetailSimplesSplit"
                },
                "home": {
                    "viewName": "Home",
                    "viewLevel": 1,
                    "viewPath": "tradelog.domain.home"
                },
                "listaCarteiras": {
                    "parent": "masterDetailSimplesSplitApp",
                    "controlId": "masterDetailSimplesSplitAppId",
                    "controlAggregation": "masterPages",
                    "viewLevel": 2,
                    "viewPath": "tradelog.domain.carteira",
                    "viewName": "ListaCarteira"
                },
                "carteira": {
                    "parent": "masterDetailSimplesSplitApp",
                    "controlId": "masterDetailSimplesSplitAppId",
                    "controlAggregation": "detailPages",
                    "viewLevel": 3,
                    "viewPath": "tradelog.domain.carteira",
                    "viewName": "Carteira"
                }
            }
        }
    }
}