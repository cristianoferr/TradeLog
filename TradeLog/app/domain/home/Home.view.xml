<core:View
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m" controllerName="tradelog.domain.home.Home"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:html="http://www.w3.org/1999/xhtml" displayBlock="true"
    >
    <Page showHeader="false" class="sapUiStdPage MasterButton" enableScrolling="false">
             <mvc:XMLView viewName="tradelog.domain.login.LoggedUser" app:title="{i18n>appTitle}"></mvc:XMLView>
<Title text="{i18n>appDescription}" />

        <TileContainer id="container" tiles="{tiles>/Tiles}" class="imagemRodapeInicio">
            <tiles>
                <StandardTile icon="{tiles>Icone}" numberUnit="" title="{tiles>Descricao}" press="onNavTo" app:rota="{tiles>Rota}"/>
            </tiles>
        </TileContainer>

    </Page>
</core:View>